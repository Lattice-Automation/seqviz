{
  "name": "seqviz",
  "version": "3.3.6",
  "private": false,
  "description": "A DNA sequence viewer. Supports custom, GenBank, FASTA, or NCBI accession input",
  "scripts": {
    "build": "rm -rf ./dist && webpack --config ./config/webpack.config.js --display minimal",
    "demo": "cd demo && npm run deploy && cd .. && git add . && git commit -m \"Update demo\"",
    "lint": "eslint src --ext ts,tsx --quiet",
    "format": "prettier ./src/** --config package.json --write && eslint src --ext ts,tsx --fix",
    "format:check": "prettier ./src/** --config package.json --check",
    "markdown": "node ./config/scripts/markdown.js ./README.md",
    "update": "npm run build && npm run markdown && git add . && git commit --amend -C HEAD && npm publish",
    "major": "npm run test:ci && npm version major && npm run update",
    "minor": "npm run test:ci && npm version minor && npm run update",
    "patch": "npm run test:ci && npm version patch && npm run update",
    "start": "PORT=8000 node ./config/scripts/start.js",
    "test": "node ./config/scripts/test.js",
    "test:ci": "npm run format:check && npm run lint && CI=true npm run test && npm run build"
  },
  "keywords": [
    "seqviz",
    "DNA",
    "viewer",
    "sequence viewer",
    "vector viewer",
    "sequence visualizer",
    "plasmid",
    "plasmid viewer"
  ],
  "homepage": "https://github.com/Lattice-Automation/seqviz",
  "bugs": {
    "url": "https://github.com/Lattice-Automation/seqviz/issues",
    "email": "contact@latticeautomation.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lattice-Automation/seqviz.git"
  },
  "license": "MIT",
  "author": "Lattice Automation (https://latticeautomation.com/)",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "dist/index.js",
  "types": "dist/viewer.d.ts",
  "unpkg": "dist/seqviz.min.js",
  "browserslist": [
    ">0.2%",
    "not dead"
  ],
  "prettier": {
    "arrowParens": "avoid",
    "importOrder": [
      "^\\w",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "printWidth": 120
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "babel-plugin-module-resolver",
      "@babel/plugin-transform-runtime"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "modulePaths": [
      "<rootDir>/src"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/setup.js"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "verbose": true
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "bufferpack": "0.0.6",
    "node-fetch": "^2.6.1",
    "prettier": "^2.5.1",
    "react-sizeme": "^3.0.0",
    "string_decoder": "^1.3.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.12.1",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/enzyme": "^3.10.10",
    "@types/jest": "^28.1.4",
    "@types/node-fetch": "^3.0.3",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/xml2js": "^0.4.9",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "camelcase": "^5.3.1",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "core-js": "^3.7.0",
    "css-loader": "^3.6.0",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "enzyme": "^3.11.0",
    "eslint": "^8.8.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^4.5.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^26.0.0",
    "mini-css-extract-plugin": "^0.8.2",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "pnp-webpack-plugin": "^1.6.4",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.2",
    "react": "^17.0.0",
    "react-dev-utils": "^11.0.0",
    "react-dom": "^17.0.0",
    "regenerator-runtime": "^0.13.7",
    "resolve": "^1.19.0",
    "resolve-url-loader": "^3.1.2",
    "source-map-loader": "^1.1.3",
    "style-loader": "^1.3.0",
    "terser-webpack-plugin": "2.2.1",
    "ts-loader": "^8.3.0",
    "ts-pnp": "^1.2.0",
    "typescript": "^4.5.2",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^2.3.0",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-node-externals": "^1.7.2",
    "workbox-webpack-plugin": "^4.3.1",
    "xml-comment-api": "^1.0.1"
  },
  "peerDependencies": {
    "react": "^16.8.6 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0"
  },
  "contributors": [
    {
      "name": "Joshua Timmons",
      "email": "jtimmons@latticeautomation.com",
      "url": "https://github.com/JJTimmons"
    },
    {
      "name": "Kevin LeShane",
      "email": "kevin@latticeautomation.com",
      "url": "https://github.com/leshane"
    },
    {
      "name": "Raymond Chung",
      "url": "https://github.com/RazzYoshi"
    }
  ]
}
