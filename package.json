{
  "name": "seqviz",
  "description": "A viewer for DNA, RNA, and protein sequences that supports many input formats",
  "version": "3.5.15",
  "main": "dist/index.js",
  "types": "dist/viewer.d.ts",
  "unpkg": "dist/seqviz.min.js",
  "scripts": {
    "build": "rm -rf ./dist && webpack --config ./config/webpack.config.js",
    "demo": "cd demo && npm run deploy && cd .. && git add . && git commit -m \"Update demo\"",
    "format": "prettier ./src/** --write && eslint src --ext ts,tsx --fix",
    "lint": "prettier ./src/** --check && eslint src --ext ts,tsx --quiet",
    "major": "./config/release.sh major",
    "minor": "./config/release.sh minor",
    "patch": "./config/release.sh patch",
    "start": "webpack serve --open --config ./config/webpack.dev.config.js",
    "test": "jest",
    "test:ci": "npm run lint && CI=true npm run test && npm run build"
  },
  "keywords": [
    "AA",
    "DNA",
    "RNA",
    "plasmid",
    "sequence",
    "seqviz",
    "vector",
    "viewer"
  ],
  "homepage": "https://github.com/Lattice-Automation/seqviz",
  "bugs": {
    "url": "https://github.com/Lattice-Automation/seqviz/issues",
    "email": "contact@latticeautomation.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lattice-Automation/seqviz.git"
  },
  "license": "MIT",
  "author": "Lattice Automation (https://latticeautomation.com/)",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "browserslist": [
    ">1%",
    "not dead"
  ],
  "dependencies": {
    "buffer": "^6.0.3",
    "bufferpack": "0.0.6",
    "react-sizeme": "^3.0.0",
    "string_decoder": "^1.3.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/enzyme": "^3.10.10",
    "@types/jest": "^28.1.4",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/xml2js": "^0.4.9",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "css-loader": "^3.6.0",
    "enzyme": "^3.11.0",
    "eslint": "^8.8.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "html-webpack-plugin": "^4.5.2",
    "jest": "^28.0.0",
    "jest-css-modules": "^2.1.0",
    "jest-environment-jsdom": "^28.1.3",
    "jest-fetch-mock": "^3.0.3",
    "path-browserify": "^1.0.1",
    "prettier": "^2.5.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "source-map-loader": "^1.1.3",
    "stream-browserify": "^3.0.0",
    "style-loader": "^1.3.0",
    "timers-browserify": "^2.0.12",
    "ts-jest": "^28.0.0",
    "ts-loader": "^8.3.0",
    "typescript": "^4.5.2",
    "url": "^0.11.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-node-externals": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^16.8.6 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.6 || ^17.0.0 || ^18.0.0"
  }
}
